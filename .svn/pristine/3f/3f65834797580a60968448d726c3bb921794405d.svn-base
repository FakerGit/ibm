.wrapper {
	width: 980px;
	margin: 0 auto;
}
/*------------- topBar开始 --------------*/
#topBar {
width:100%;
	height: 33px;
	background: #292929;
	color: #ddd;
	line-height: 33px;
}

#topBar .l {
	float: left;
}

#topBar .r {
	float: right;
	width: 400px;
	height: 33px;
	font-family: "宋体;";
	text-align: right;
	padding-right: 5px;
}

#topBar .r a {
	color: #fff;
	margin: 0px 5px
}

#topBar .r a:hover {
	text-decoration: underline;
	color: #eee;
}

#topBar .r span {
	margin-left: 15px
}
/* ----------------------topBar结束--------------------- */

/* -----------------header logo开始-------------------------- */
#header {
	height: 220px;
}

#header .wrapper {
	position: relative;
}

#header .logo {
	position: absolute;
	width: 450px;
	height: 160px;
}
/* -----------------header logo结束-------------------------- */

/* -----------------nav导航开始-------------------------- */
#nav {
	width: 980px;
	margin: 0 auto;
}

#nav ul {
	height: 50px;
	overflow: hidden;
	background: url(../images/nav1.png) no-repeat;
	padding-left: 0px;
}

#nav ul li {
	width: 140px;
	float: left;
	height: 20px;
	display: inline;
}

#nav ul li a {
	display: block;
	height: 50px;
	line-height: 50px;
	text-indent: -10000px;
	background: url(../images/nav1.png) no-repeat
}

#nav ul li .a1 {
	background-position: -0px 0
}

#nav ul li .a1:hover {
	background-position: -0px -50px
}

#nav ul li .a2 {
	background-position: -140px 0
}

#nav ul li .a2:hover {
	background-position: -140px -50px
}

#nav ul li .a3 {
	background-position: -280px 0
}

#nav ul li .a3:hover {
	background-position: -280px -50px
}

#nav ul li .a4 {
	background-position: -420px 0
}

#nav ul li .a4:hover {
	background-position: -420px -50px
}

#nav ul li .a5 {
	background-position: -560px 0
}

#nav ul li .a5:hover {
	background-position: -560px -50px
}

#nav ul .search {
	width: 280px;
	float: left;
	display: inline;
}

#nav ul .search p {
	height: 30px;
	margin: 11px 0px;
	padding-right: 40px;
	float: right;
	overflow: hidden;
}

#nav ul .search p .searchTxt {
	margin: 0px;
	width: 170px;
	border: none;
	float: left;
	height: 28px;
	background: no-repeat;
	font-size: 16px;
}

#nav ul .search p .search_icon {
	width: 28px;
	height: 28px;
	margin: 0px;
	float: left;
}
/* -----------------nav导航结束-------------------------- */

/* -----------------main开始-------------------------- */
#main {
	background: #fff;
	padding: 15px;
	width: 950px;
	margin: 0 auto;
	border-radius: 13px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 13px;
	overflow: hidden
}
/* -----------------footer开始-------------------------- */
#footer {
